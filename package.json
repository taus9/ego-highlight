{
  "name": "ego-highlight",
  "displayName": "Ego Syntax Highlighting",
  "description": "Syntax highlighting for the Ego programming language",
  "publisher": "Tausy",
  "version": "0.1.0",
  "engines": { "vscode": "^1.85.0" },
  "categories": ["Programming Languages"],
  "configuration": "./language-configuration.json",
  "contributes": {
    "languages": [
      {
        "id": "ego",
        "aliases": ["Ego", "ego"],
        "extensions": [".ego"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ego",
        "scopeName": "source.ego",
        "path": "./syntaxes/ego.tmLanguage.json"
      }
    ]
  },
  "keywords": ["syntax", "highlighting", "ego", "language"],
  "license": "MIT",
  "author": {
    "name": "Christopher Martinez",
    "email": "crmarz@proton.me",
    "github": "github.com/taus9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taus9/ego-highlight.git"
  }
}